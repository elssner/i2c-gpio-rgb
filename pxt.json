{
    "name": "rgb-8io",
    "version": "0.0.1",
    "dependencies": {
        "core": "*",
        "bit": "github:calliope-net/bit#v1.0.6",
        "gpio": "github:calliope-net/gpio#v1.1.2"
    },
    "files": [
        "main.blocks",
        "main.ts",
        "README.md"
    ],
    "testFiles": [
        "test.ts"
    ],
    "targetVersions": {
        "target": "4.0.29",
        "targetId": "calliopemini"
    },
    "supportedTargets": [
        "calliopemini",
        "microbit"
    ],
    "preferredEditor": "blocksprj",
    "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "pairing_mode": 1,
                    "partial_flashing": 1,
                    "dfu_service": 1,
                    "event_service": 1,
                    "device_info_service": 1
                },
                "stack_size": 2048,
                "gatt_table_size": "0x800",
                "sram_end": "0x20008000",
                "RAM_SIZE": "\"32K\""
            }
        }
    }
}
